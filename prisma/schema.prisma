generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Vaga {
  id                 String   @id @default(uuid())
  titulo             String
  localizacao        String
  descricao          String
  responsabilidades  String
  requisitos         String
  beneficios         String
  remuneracao        String
  area               String
}